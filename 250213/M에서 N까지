#include <stdio.h>

void main() {
    
    int M, N;
    printf("M에서 N까지 출력할 값을 입력해주세요\n");
    scanf("%d %d\\n", &M, &N);
    if (M > N) {
        printf("M은 N보다 작은 숫자를 입력해 주세요");
    } else {
        for (int i = M; i < N + 1; i++) {
            int j;
            for (j = i; j > 0 && j % 10 != 7; j /= 10);
            if (i % 7 == 0 || j != 0) {
                continue;
            }
            printf("%d\n", i);
            if (i % 5 == 0) {
                break;
            }
        }
    }
}    

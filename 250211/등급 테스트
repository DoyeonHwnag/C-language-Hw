#include <stdio.h>

void main(){
    
    int a = 0;
    scanf("%d", &a);
    if(a >= 90){
        printf("A등급", 1.1 * a);
    }else if(a >= 80){
        printf("B등급", 1.05 * a);
    }else if(a >= 70){
        printf("C등급", a);
    }else if(a >= 60){
        printf("D등급", a);
    }else{
        printf("F등급", a);
    }
}

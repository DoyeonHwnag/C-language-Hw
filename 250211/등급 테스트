#include <stdio.h>
#include <string.h>

void main(){
    
    int a = 0;
    char * grade;
    scanf("%d", &a);
    if(a >= 90){
        grade = "A등급";
        printf("%s: %f", grade,1.1 * a);
    }else if(a >= 80){
        grade = "B등급";
        printf("%s: %f", grade, 1.05 * a);
    }else if(a >= 70){
        grade = "C등급";
        printf("%s: %d", grade, a);
    }else if(a >= 60){
        grade = "D등급";
        printf("%s: %d", grade, a);
    }else{
         grade = "F등급";
        printf("%s: %d", grade, a);
    }
}

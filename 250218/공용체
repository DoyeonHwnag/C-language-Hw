#include <stdio.h>

void main() {
    union Value {
        int intVal;
        float floatVal;
        char charVal;
    };
    
    union Value v;
    v.intVal = 123;
    v.floatVal =1.32;
    v.charVal = 'A';
    
    printf("%d %d\n", v.intVal, &v.intVal);
    printf("%d %d\n", v.floatVal, &v.floatVal);
    printf("%c %d", v.charVal, &v.charVal);
}

#include <stdio.h>

int main() {
    int a, b, c; 
    
    float d;
    printf("숫자 세개를 띄어쓰기 해서 입력하세요");
    scanf("%d", &a);
    scanf("%d", &b);
    scanf("%d", &c);
    if(c != 0) {
        d = (a + b) * c - (a - b)/(float) c;
        printf("%f", d);
    }
    else
        printf("c는 0이되면 안됩니다");
}
